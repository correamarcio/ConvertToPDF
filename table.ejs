<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Table</title>
  </head>
  <body>
    <table>
      <thead>
        <% header.forEach((element)=>{ %>
        <th><%= element %></th>
        <% }) %>
      </thead>
      <tbody>
        <% body.forEach((element)=>{ %>
        <tr>
          <% element.forEach(e=>{ %>
          <td><%=e%></td>
          <%})%>
        </tr>
        <%})%>
      </tbody>
    </table>
  </body>
</html>
